groups:
- name: nginx
  rules:
  - alert: "nginxDown"
    expr: nginx_up == 0
    for: 5m
    labels:
      severity: "严重"
    annotations:
      value: "{{ $value }}"
      summary: "nginx {{ $labels.instance }} down"
      description: "{{ $labels.instance }} of job {{ $labels.job }} has been down for more than 5 minutes."
