groups:
- name: process
  rules:
  - alert: "namedprocess_scrape_errors"
    expr: namedprocess_scrape_errors == 1
    for: 5m
    labels:
      severity: "严重"
    annotations:
      value: "{{ $value }}"
      summary: "process {{ $labels.instance }} scrape error"
      description: "{{ $labels.instance }} of job {{ $labels.job }} has been scrape error for more than 5 minutes."