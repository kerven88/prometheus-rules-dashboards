groups:
- name: blackbox
  rules:
  - alert: "probe_success"
    expr: probe_success == 0
    for: 5m
    labels:
      severity: "严重"
    annotations:
      value: "{{ $value }}"
      summary: "blackbox {{ $labels.instance }} probe failed"
      description: "{{ $labels.instance }} of job {{ $labels.job }} has been probe failed for more than 5 minutes."
